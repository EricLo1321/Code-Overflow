<!DOCTYPE html>
<html>
  <head>
    <title>StackOverflowClone</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all'%>
    <%= javascript_include_tag 'application'%>

    <link rel="stylesheet" href="//cdn.quilljs.com/1.2.6/quill.snow.css">
    <script>
      window.logo = "<%= asset_path('logo.png')%>";
      window.navLogo = "<%= asset_path('logo2.png')%>";
      window.dropdown = "<%= asset_path('dropdown.png')%>";
    </script>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
